<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <!-- <link rel='stylesheet' href='/stylesheets/style.css' /> -->
     <!-- <script type="text/javascript" src="ejs_production.js"></script>  -->
    <style>
      .container {
          margin:100px 500px 100px 75px;
          font-size:14px;
      }
    </style>
  </head>
  <body>
    <div class = "container">

      <!--Displayig the contents of database-->
      <table class="table table-striped table-bordered table-hover table-condensed">
        <thead>
          <th>Name</th>
          <th>Domain</th>
        </thead>
        <tbody>
          <% for(let i = 0; i < title.length; i++) { %>
            <tr>
              <td><%= title[i].Name %></td>
              <td><%= title[i].Domain %></td>
            </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </body>
</html>
